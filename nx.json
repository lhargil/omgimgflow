{
  "npmScope": "omgimgflow",
  "affected": { "defaultBase": "master" },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": { "cacheableOperations": ["build", "lint", "test", "e2e"] }
    }
  },
  "projects": {
    "omgimgflow-app": { "tags": ["scope:omgimgflow-app", "type:app"] },
    "omgimgflow-app-e2e": { "tags": [], "implicitDependencies": ["omgimgflow-app"] },
    "omgimgflow-app-shell": { "tags": ["scope:omgimgflow-app", "type:feature"] },
    "omgimgflow-app-photos-shell": { "tags": ["scope:omgimgflow-app", "type:feature"] },
    "omgimgflow-app-photos-feature-list": { "tags": ["scope:omgimgflow-app", "type:feature"] },
    "omgimgflow-app-photos-data-access": { "tags": ["scope:omgimgflow-app", "type:data-access"] },
    "omgimgflow-app-photos-feature-edit": { "tags": ["scope:omgimgflow-app", "type:feature"] }
  }
}
